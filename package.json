  "engines": {
    "node": "^10.13 || 12 || 14 || 15",
    "npm": ">=6"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.6.2",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.5",
    "chalk": "4.1.0",
    "date-fns": "2.25.0",
    "enzyme": "3.11.0",
    "history": "5.0.1",
    "js-cookie": "3.0.1",
    "msw": "0.35.0",
    "pac": "1.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "4.3.1",
    "react-loader-spinner": "4.0.0",
    "react-router-dom": "5.3.0",
    "react-slick": "0.28.1",
    "react-slick-slider": "0.15.10",
    "slick-carousel": "1.8.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "gh-pages": "5.0.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix src/",
    "format": "prettier --write \"./src\"",
    "run-all": "npm-run-all --parallel test lint:fix"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix"
    ],
    "*.{js, jsx, json, html, css}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "description": "In this project let's build a **Movies App** by applying the concepts we have learned till now. This project allows you to practice the concepts and techniques learned till React Course and apply them in a concrete project.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Syam-Narendra/Netflix-Clone-React.git"
